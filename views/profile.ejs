<!--User Profile Information-->

<div class="profile-header text-center"
     style="background-image: url(../assets/img/iceland.jpg);">
    <div class="container">
        <div class="container-inner">
            <% console.log(user_profile_picture); %>
            <img style="width: 200px; height: 190px;" class="img-thumbnail media-object" src="../public/users/<%= username %>/profile_picture/<%= user_profile_picture %>">
            <h3 class="profile-header-user"> <%= username; %> </h3>

        </div>
    </div>

    <nav class="profile-header-nav">
        <ul class="nav nav-tabs">

            <li id="bio" class="">
                <a class="bio" href="#">Bio</a>
            </li>

            <li id="photos" class="">
                <a class="photos" href="#">Photos</a>
            </li>

            <li id="friends" class="">
                <a class="friends" href="#">Friends</a>
            </li>

        </ul>
    </nav>
</div>

<div class="container m-y-md bio_panel">

    <div class="jumbotron">
    <%if(bio){ %>

    <p class="profile-header-bio">
        <%- bio;%>
    </p>

    <%}else{%>

        <p class="profile-header-bio">
            Bio hasn't been filled out.
        </p>
    <%}%>
</div>

</div><!--END OF BIO CONTAINER-->
